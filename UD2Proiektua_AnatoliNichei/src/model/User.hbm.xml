<hibernate-mapping schema="chinook">
<class entity-name="model.User" name="model.User" table="users">
<id column="UserId" name="id">
<generator class="increment"/>
</id>
<property name="izena" column="UserName"/>
<property name="pasahitza" column="Password"/>
<property name="rola" column="Role"/>
</class>
</hibernate-mapping>