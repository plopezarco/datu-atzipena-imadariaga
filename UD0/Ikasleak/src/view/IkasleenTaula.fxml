<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="651.0" styleClass="mainFxmlClass" stylesheets="@DarkThemeArgitua.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.IkasleenTaulaController">
      
    <Label fx:id="izenburua" layoutX="12.0" layoutY="6.0" styleClass="" stylesheets="" text="Ikasleen Zerrenda" />
    <TableView fx:id="tableviua" editable="true" layoutX="12.0" layoutY="70.0" prefHeight="200.0" prefWidth="630.0">
        <columns >
            <TableColumn fx:id="zenbakiaCol" prefWidth="150.0" text="Zenbakia" styleClass="lehen-zutabe" />
            <TableColumn fx:id="izenaCol" prefWidth="150.0" text="Izena" />
            <TableColumn fx:id="abizena1Col" prefWidth="200.0" text="Abizena" />
        </columns>
    </TableView>
    <HBox fx:id="botoiak" layoutX="12.0" layoutY="270.0" prefHeight="100.0" prefWidth="630.0">
        <children>
            <TextField fx:id="addZenbakia" prefWidth="150.0" />
            <TextField fx:id="addIzena" prefWidth="150.0" />
            <TextField fx:id="addAbizena" prefWidth="150.0" />
            <Button fx:id="gehitu" mnemonicParsing="false" onAction="#handleGehituAction" text="  Gehitu   " />
            <Button fx:id="ezabatu" mnemonicParsing="false" onAction="#handleEzabatuAction" prefHeight="33.0" prefWidth="94.0" text="Ezabatu" />
        </children>
    </HBox>  
</AnchorPane>
